{
	"env": {
		"node":true,
		"browser":true
	},
	"root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
		"@typescript-eslint",
		"prettier"
  ],
  "extends": [
    "airbnb-base",
		"plugin:@typescript-eslint/recommended"
	],
	"settings": {
    "import/resolver": {
      "node": {
				"paths": ["src"],
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
	"rules": {
		"no-console":"off",
		"linebreak-style":"off",
		"comma-dangle":"off",
		"object-curly-newline":"off",
		"wrap-iife":"off",
		"import/extensions": "off",
		"prettier/prettier": "error"
	}
}